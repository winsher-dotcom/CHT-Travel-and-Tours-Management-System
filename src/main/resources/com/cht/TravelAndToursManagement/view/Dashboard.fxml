<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="DashboardContainer" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cht.TravelAndToursManagement.client.controller.DashboardController">
    <left>
        <AnchorPane prefHeight="360.0" prefWidth="140.0" BorderPane.alignment="CENTER">
            <children>
                <Button layoutX="2.0" layoutY="29.0" mnemonicParsing="false" onAction="#addBooking" prefWidth="138.0"
                        text="Add Booking"/>
                <Button layoutX="2.0" layoutY="60.0" mnemonicParsing="false" onAction="#homeButton" prefHeight="25.0"
                        prefWidth="139.0" text="Home"/>
                <Button layoutX="1.0" layoutY="91.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="138.0"
                        text="Packages"/>
            </children>
        </AnchorPane>
    </left>
    <top>
        <AnchorPane prefHeight="40.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Label layoutX="191.0" layoutY="12.0" text="CHT Travel &amp; Tours Management System"/>
            </children>
        </AnchorPane>
    </top>
</BorderPane>
